<?xml version="1.0"?>
<!-- initial phpunit configuration file that you can modify for your project needs -->
<phpunit cacheTokens="true"
	colors="true"
	convertErrorsToExceptions="true"
	convertNoticesToExceptions="true"
	convertWarningsToExceptions="true"
	stopOnError="false"
	stopOnFailure="false"
	stopOnIncomplete="false"
	stopOnSkipped="false"
	strict="false"
	verbose="false">
	<logging>
		<log type="coverage-html" target="phpunit/coverage" charset="UTF-8"
			yui="true" highlight="false"
			lowUpperBound="35" highLowerBound="70"/>
		<log type="coverage-clover" target="phpunit/coverage.xml"/>
		<log type="junit" target="phpunit/junit.xml" logIncompleteSkipped="false"/>
	</logging>
</phpunit>
