<?xml version="1.0"?>
<phpunit cacheTokens="true"
	colors="true"
	convertErrorsToExceptions="true"
	convertNoticesToExceptions="true"
	convertWarningsToExceptions="true"
	stopOnError="false"
	stopOnFailure="false"
	stopOnIncomplete="false"
	stopOnSkipped="false"
	strict="true"
	verbose="true"
	timeoutForSmallTests="10">
	<logging>
		<log type="coverage-html" target="build/phpunit/coverage" charset="UTF-8" highlight="true" lowUpperBound="35" highLowerBound="70"/>
		<log type="coverage-clover" target="build/phpunit/coverage.xml"/>
		<log type="junit" target="build/phpunit/junit.xml" logIncompleteSkipped="true"/>
		<log type="tap" target="build/phpunit/testresults.tap"/>
	</logging>
</phpunit>
